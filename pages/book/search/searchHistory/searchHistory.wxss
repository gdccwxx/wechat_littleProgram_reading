.searchHistoryChild {
  width: 99%;
  height: 90rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background-color: #ffffff;
  border-bottom: 2rpx solid #f3f5f5;
  line-height: 90rpx;
  font-size: 16px;
}
.searchHistoryText{
  margin-top: 50rpx;
  vertical-align: middle;
  color: #051414;
}
.searchHistoryChild .clock{
  width:28rpx;
  height: 28rpx;
  margin-right: 15rpx;
  vertical-align: middle;
}
.searchHistoryChild .cancel {
  width: 25rpx;
  height: 25rpx;
  margin-top: 40rpx;
}