.container {
  width: 94%;
  margin: 0 auto;
}

.editer {
  width: 100%;
  height: 90rpx;
  border-bottom: 15rpx solid #f6f7f7;
}

.editer .container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.infoAll {
  width: 200rpx;
  height: 100%;
  line-height: 90rpx;
  font-size: 25rpx;
  color: #000;
  font-weight: bold;
}

.editBox {
  width: 100rpx;
  height: 45rpx;
  background-color: #f2f4f4;
  color: #565c5c;
  border-radius: 10rpx;
  font-size: 25rpx;
  text-align: center;
  line-height: 45rpx;
  margin-top: 22rpx;
}

.searchResultChild {
  width: 100%;
  height: 248rpx;
  border-bottom: 2rpx solid #eef5f5;
  display: flex;
  flex-direction: row;
}

.bookCoverImage {
  width: 155rpx;
  height: 200rpx;
  margin-top: 18rpx;
  margin-right: 30rpx;
}

.resultInfo {
  margin-top: 18rpx;
  width: 450rpx;
}

.resultInfo text {
  display: block;
}
.have{
  display: inline !important;
}

.Infos {
  font-size: 24rpx;
  color: #b8baba;
  margin-top: 10rpx;
}

.InfoName {
  font-size: 30rpx;
  color: #000;
}

.InfoHave {
  font-size: 25rpx;
  color: #000;
  margin-top: 10rpx;
}

.deleteIcon {
  width: 40rpx;
  height: 40rpx;
  margin-top: 102rpx;
  animation: size 1s;
}
.textClass{
  animation: opacity 1s;
}
@keyframes size {
  from {
    transform: rotate(0) scale(0);
  }
	to {
    transform: rotate(360deg) scale(1);
  }
}
@keyframes opacity {
  from {
    opacity: 0;
  }
	to {
    opacity: 1;
  }
}